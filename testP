%%the beginning of an test data for Micro/Ex
Program testP
Begin
    declare I,J as integer;
    declare A,B,C,D, LLL[100] as float;
FOR (I:=2*J-4 DOWNTO 100*J+6)
    A:=-LLL[I]+B*D-C;
ENDFOR
I:=1;
WHILE (I<=100)
    IF (A>=10000.0) THEN
        print(A+3.14);
    ELSE   
        print(2,1.4);
    ENDIF
    I:=I+1;
ENDWHILE
End